{
	"enableAutoDiscovery": true,
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "None",
		"moduleResolution": "node",
		"newLine": "LF",
		"removeComments": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true
	}
}
